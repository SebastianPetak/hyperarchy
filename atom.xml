---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Hyperarchy</title>
 <link href="{{site.base_url}}/atom.xml" rel="self"/>
 <link href="{{site.base_url}}"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://hyperarchy.com/</id>
 <author>
   <name>Nathan Sobo</name>
   <email>nathan@hyperarchy.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{site.base_url}}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{site.base_url}}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>